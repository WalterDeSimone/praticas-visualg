algoritmo "Exercício 04"
var
   Valor_Carro: Real
   
inicio
      Escreva("Digite o valor do carro: R$ ")
      Leia(Valor_Carro)
      
      se (Valor_Carro >= 92000) entao
         Escreval("Carro considerado de alto padrao")
      senao
           Escreval("Carro nao e considerado de alto padrao")
      fimse

fimalgoritmo
