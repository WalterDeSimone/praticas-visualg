algoritmo "Exercício 08"
var
   N1, N2, Resto: Inteiro

inicio
      Escreva("Insira o numero A: ")
      Leia(N1)
      Escreva("Insira o numero B: ")
      Leia(N2)

      Resto <- N1 % N2

      Escreva("Numero A eh divisivel pelo numero B? ")
      Escreval(Resto = 0)


fimalgoritmo
