algoritmo "Exercício 09"
var
   N1, N2, N3: Real

inicio
      Escreva("Insira o numero A: ")
      Leia(N1)
      Escreva("Insira o numero B: ")
      Leia(N2)
      Escreva("Insira o numero C: ")
      Leia(N3)

      Escreva("Numeros diferentes? ")
      Escreva((N1 <> N2) E (N1 <> N3) E (N2 <> N3))

fimalgoritmo

