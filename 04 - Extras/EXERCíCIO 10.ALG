algoritmo "Exercício 10"
var
   N1, N2, N3: Real

inicio
      Escreva("Digite primeiro numero: ")
      Leia(N1)
      Escreva("Digite segundo numero: ")
      Leia(N2)
      Escreva("Digite terceiro numero: ")
      Leia(N3)

      Escreva("Numeros diferentes? ")
      Escreva((N1 <> N2) ou (N2 <> N3) ou (N1 <> N3))
fimalgoritmo

