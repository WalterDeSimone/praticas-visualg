algoritmo "Exercício 01"
var
   cont: Inteiro
inicio
      Escreval ("Os numeros inteiros de 1 a 200 impares, sao:")
      cont <- 1
      Enquanto (cont <= 200) Faca
        Escreva (cont, " ; ")
        cont <- cont + 2
      FimEnquanto
      Escreval (" ")
      Escreval ("Fim da contagem")

fimalgoritmo