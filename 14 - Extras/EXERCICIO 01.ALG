algoritmo "Exercicio 01"
var
   n: vetor[0..7] de inteiro
   i: inteiro
inicio
      n[0] <- 1
      n[1] <- 15
      n[2] <- 111
      n[3] <- 19
      n[4] <- 17
      n[5] <- 14
      n[6] <- 134
      n[7] <- 17

      Para i <- 0 ate 7 faca
           Escreval ("n[", i , "] -> ", n[i])

      FimPara

fimalgoritmo
